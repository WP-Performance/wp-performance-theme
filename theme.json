{
  "$schema": "https://schemas.wp.org/wp/6.0/theme.json",
  "customTemplates": [],
  "settings": {
    "appearanceTools": true,
    "color": {
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "link": false,
      "duotone": [
        {
          "colors": ["#000000", "#ffffff"],
          "name": "Foreground and background",
          "slug": "foreground-and-background"
        },
        {
          "colors": ["#000000", "#707C84"],
          "name": "Foreground and secondary",
          "slug": "foreground-and-secondary"
        },
        {
          "colors": ["#000000", "rgb(190, 24, 93)"],
          "name": "Foreground and accent",
          "slug": "foreground-and-accent"
        },
        {
          "colors": ["#261578", "#ffffff"],
          "name": "Primary and background",
          "slug": "primary-and-background"
        },
        {
          "colors": ["#261578", "#707C84"],
          "name": "Primary and secondary",
          "slug": "primary-and-secondary"
        },
        {
          "colors": ["#261578", "rgb(190, 24, 93)"],
          "name": "Primary and accent",
          "slug": "primary-and-accent"
        }
      ],
      "gradients": [],
      "palette": [
        {
          "color": "#000000",
          "name": "Black",
          "slug": "black"
        },
        {
          "color": "#444444",
          "name": "Foreground",
          "slug": "foreground"
        },
        {
          "color": "#ffffff",
          "name": "Background",
          "slug": "background"
        },
        {
          "color": "#261578",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#707C84",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "rgb(190, 24, 93)",
          "name": "Accent",
          "slug": "accent"
        }
      ]
    },
    "custom": {
      "sizes": {
        "content": "1024px"
      },
      "colors": {
        "body": "#f4f5f6"
      },
      "spacing": {
        "large": "clamp(4rem, 10vw, 8rem)",
        "medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
        "outer": "var(--wp--custom--spacing--small, 1.25rem)",
        "small": "max(1.25rem, 5vw)"
      },
      "typography": {
        "font-size": {
          "colossal": "clamp(3.25rem, 8vw, 6.25rem)",
          "gigantic": "clamp(2.75rem, 6vw, 3.25rem)",
          "huge": "clamp(2.25rem, 4vw, 2.75rem)"
        },
        "line-height": {
          "medium": 1.3999999999999999,
          "normal": 1.6000000000000001,
          "small": 1.2,
          "tiny": 1.1499999999999999
        }
      }
    },
    "layout": {
      "contentSize": "var(--wp--custom--sizes--content)",
      "wideSize": "100%"
    },
    "spacing": {
      "padding": false,
      "margin": false,
      "units": ["%", "rem", "vh", "vw"]
    },
    "typography": {
      "customFontSize": false,
      "textDecoration": false,
      "lineHeight": false,
      "letterSpacing": false,
      "fontWeight": false,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Sora",
              "fontStretch": "25% 151%",
              "fontWeight": "100 800",
              "fontStyle": "oblique 0deg 10deg",
              "unicodeRange": "U+000-5FF;",
              "src": ["file:./assets/fonts/Sora-VariableFont_wght.woff2"]
            }
          ],
          "fontFamily": "\"Sora\", sans-serif",
          "name": "Sora",
          "slug": "Sora"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Lora",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/Lora-Regular.woff2"]
            },
            {
              "fontFamily": "Lora",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/Lora-Italic.woff2"]
            },
            {
              "fontFamily": "Lora",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/Lora-BoldItalic.woff2"]
            },
            {
              "fontFamily": "Lora",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/Lora-Bold.woff2"]
            }
          ],
          "fontFamily": "\"Lora\", serif",
          "name": "Lora",
          "slug": "Lora"
        }
      ],
      "fontSizes": [
        {
          "size": "1rem",
          "slug": "small"
        },
        {
          "size": "1.125rem",
          "slug": "medium"
        },
        {
          "size": "1.75rem",
          "slug": "large"
        },
        {
          "size": "clamp(1.75rem, 3vw, 2.25rem)",
          "slug": "x-large"
        }
      ]
    }
  },
  "styles": {
    "blocks": {
      "core/button": {
        "border": {
          "radius": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--accent)",
          "text": "var(--wp--preset--color--background)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--custom--typography--font-size--gigantic)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--tiny)"
        },
        "color": { "text": "var(--wp--preset--color--accent)" }
      },
      "core/quote": {
        "border": {
          "width": "3px",
          "color": "var(--wp--preset--color--secondary)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontStyle": "normal",
          "fontWeight": "normal",
          "lineHeight": "var(--wp--custom--typography--line-height--normal)"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--foreground)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--custom--typography--font-size--colossal)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--tiny)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--custom--typography--font-size--gigantic)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--small)"
        },
        "color": { "text": "var(--wp--preset--color--black)" }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--custom--typography--font-size--huge)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--tiny)"
        },
        "color": { "text": "var(--wp--preset--color--black)" }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--tiny)"
        },
        "color": { "text": "var(--wp--preset--color--black)" }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--normal)"
        },
        "color": { "text": "var(--wp--preset--color--black)" }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--normal)"
        },
        "color": { "text": "var(--wp--preset--color--black)" }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--accent)"
        }
      }
    },
    "spacing": {
      "blockGap": "1.5rem"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--lora)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "var(--wp--custom--typography--line-height--normal)"
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "version": 2
}
