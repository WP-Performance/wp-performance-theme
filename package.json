{
  "name": "press-wind-fse",
  "version": "0.0.1",
  "description": "Wordpress theme base with Tailwind CSS and Vite JS for FSE",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env APP_ENV=development VITE_CJS_TRACE=true bunx vite & bun run watch:editor",
    "build": "APP_ENV=production bunx vite build && bun run build:editor && bun run build:admin",
    "dev:admin": "cross-env APP_ENV=development bunx vite --config admin/vite.config.js",
    "build:admin": "APP_ENV=production bunx vite build --config admin/vite.config.js",
    "watch:editor": "APP_ENV=development IS_EDITOR=true node_modules/.bin/tailwindcss  --watch --verbose --no-map -i assets/css/styles.css -o style-editor.css --postcss -p",
    "build:editor": "NODE_ENV=production IS_EDITOR=true node_modules/.bin/tailwindcss  -i assets/css/styles.css -o style-editor.css --postcss -m",
    "favicon": "node ./inc/faviconGenerator.mjs",
    "php:lint": "./vendor/bin/duster lint",
    "php:fix": "./vendor/bin/duster fix"
  },
  "author": "Faramaz Patrick<info@goodmotion.fr>",
  "license": "ISC",
  "trustedDependencies": [
    "sharp"
  ],
  "devDependencies": {
    "@_tw/themejson": "^0.1.2",
    "@babel/plugin-transform-arrow-functions": "^7.23.3",
    "@tailwindcss/forms": "^0.5.7",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@wordpress/browserslist-config": "^5.29.0",
    "@wordpress/components": "^25.12.0",
    "@wordpress/compose": "^6.23.0",
    "@wordpress/data": "^9.16.0",
    "@wordpress/edit-post": "^7.23.0",
    "@wordpress/element": "^5.23.0",
    "@wordpress/eslint-plugin": "^17.3.0",
    "@wordpress/i18n": "^4.46.0",
    "@wordpress/icons": "^9.37.0",
    "@wordpress/plugins": "^6.14.0",
    "@wordpress/scripts": "^26.17.0",
    "@wordpress/stylelint-config": "^21.29.0",
    "autoprefixer": "^10.4.16",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.22.2",
    "classnames": "^2.3.2",
    "core-js": "^3.33.2",
    "cross-env": "^7.0.3",
    "exec-php": "^0.0.6",
    "favicons": "^7.1.4",
    "lightningcss": "^1.22.1",
    "lightningcss-cli": "^1.22.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.3.0",
    "postcss-url": "^10.1.3",
    "prettier": "^3.1.0",
    "sharp": "^0.33.0",
    "tailwindcss": "3.3.5",
    "terser": "^5.24.0",
    "vite": "5.0.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-live-reload": "^3.0.2"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "^0.13.6",
    "algoliasearch": "^4.20.0",
    "instantsearch.css": "^8.1.0",
    "instantsearch.js": "^4.60.0",
    "tailwind-scrollbar": "^3.0.5"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
